async function insertMultiple () {
    await Inserts.bulkCreate(json).then(async ()=>{
        return await Inserts.findAll()
    }).then((data)=>{
        console.log('insertados')
    })
}

Agregar campos nuevos 
const array1 =
[
    {
        data:'hola 1',
        boolean: true

    },
    {
        data:'hola 2',
        boolean: true

    },
    {
        data:'hola 3',
        boolean: true

    }
];

array1.forEach((value, index) => {
    
    let datosExtra={
        valor:'',
        Cantidad: ''
    };

    let dato=array1[index];

    
        datosExtra.valor = index*2
        datosExtra.Cantidad = (8*index)+10
   


    addElement(datosExtra, dato);

    function addElement(elementlist, element){
        let newList = Object.assign(element,elementlist)
        return console.log(newList);
    }

    
    
});


query

create table productos
(
   _id int not null,
   descripcion char(30) not null,
   id_prov int,
   cantidad int not null, 
   categoria int not null,
   precio_comp double not null, 
   precio_ven double not null	
   
);


create table ventas
(
	consecutivo int not null,
	des_venta varchar(500),
	id_product int not null,
	id_cliente int not null,
	valor_venta double not null,
	fecha_venta date
);

create table usuarios 
(
	_cel int not null,
	nombre varchar(16),
	apellido varchar(16),
	f_nacimiento date,
	role int not null
);

create table categorias
(
	id_cat int not null,
	descrip varchar(20)
);

create table roles
(
	id_rol int not null,
	descrip_rol varchar(30)
);

create table proveedores 
(
	id_provee int not null,
	descripcion varchar(30)
)

